{
  "name": "broilerplate",
  "description": "A boilerplate with SASS, JS & Vagrant.",
  "version": "1.4.2",
  "repository": {
    "type": "git",
    "url": "git@github.com:fboes/broilerplate.git"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "beepbeep": "^1.2.0",
    "gulp": "^3.9.1",
    "gulp-appcache": "0.0.4",
    "gulp-clone": "^1.0.0",
    "gulp-concat": "^2.6.0",
    "gulp-jshint": "^2.0.0",
    "gulp-livereload": "^3.8.1",
    "gulp-plumber": "^1.1.0",
    "gulp-postcss": "^6.1.0",
    "gulp-rename": "^1.2.2",
    "gulp-replace": "^0.5.4",
    "gulp-sass": "^2.3.1",
    "gulp-sourcemaps": "^2.0.0-alpha",
    "gulp-uglify": "^1.5.3",
    "jshint": "^2.9.2",
    "rtlcss": "^2.0.3"
  },
  "config": {
    "develop": false
  },
  "directories": {
    "doc": "docs",
    "lib": "src",
    "htdocs": "htdocs",
    "template": "htdocs",
    "sass": "src/sass",
    "css": "htdocs/css",
    "js_src": "src/js-src",
    "js": "htdocs/js",
    "images": "htdocs/images",
    "build": "build"
  },
  "scripts": {
    "build": "gulp",
    "start": "gulp watch",
    "deploy-live": "build/deploy.sh live",
    "vagrant-up": "build/vagrant && vagrant up && cd -",
    "vagrant-suspend": "build/vagrant && vagrant suspend && cd -"
  },
  "author": "Frank BoÃ«s",
  "license": "MIT",
  "private": false
}
