{
  "name": "broilerplate",
  "description": "A boilerplate with SASS, JS & Vagrant.",
  "version": "1.4.1",
  "repository": {
    "type": "git",
    "url": "git@github.com:fboes/broilerplate.git"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "grunt": "^1.0.1",
    "grunt-appcache": "^0.2.0",
    "grunt-contrib-jshint": "^1.0.0",
    "grunt-contrib-sass": "^1.0.0",
    "grunt-contrib-uglify": "^1.0.1",
    "grunt-contrib-watch": "^1.0.0",
    "grunt-postcss": "^0.8.0",
    "grunt-shell": "^1.2.1",
    "grunt-text-replace": "^0.4.0",
    "load-grunt-tasks": "^3.5.0",
    "rtlcss": "^2.0.3"
  },
  "config": {
    "develop": false
  },
  "directories": {
